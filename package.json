{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "yarn chakra:generate-theme-typings && yarn graphql:generate:client && yarn graphql:generate:server",
    "typecheck": "tsc --noEmit",
    "chakra:generate-theme-typings": "chakra-cli tokens src/theme/theme.ts",
    "graphql:generate:client": "graphql-codegen --config graphql-codegen-client.yml",
    "graphql:generate:server": "graphql-codegen --config graphql-codegen-server.yml",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts"
  },
  "dependencies": {
    "@apollo/client": "3.8.7",
    "@chakra-ui/pro-theme": "0.10.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@envelop/core": "5.0.0",
    "@envelop/sentry": "7.0.0",
    "@graphql-tools/merge": "9.0.0",
    "@graphql-tools/schema": "10.0.0",
    "@graphql-tools/utils": "10.0.8",
    "@hookform/resolvers": "3.3.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/nextjs": "7.80.1",
    "@sentry/node": "7.80.1",
    "@sentry/tracing": "7.80.1",
    "@sentry/types": "7.80.1",
    "@svgr/webpack": "8.1.0",
    "@types/graphql-fields": "1.3.9",
    "axios": "1.6.2",
    "class-validator": "0.14.0",
    "date-fns": "2.30.0",
    "drizzle-orm": "^0.30.10",
    "framer-motion": "10.16.5",
    "graphql": "16.8.1",
    "graphql-fields": "2.0.3",
    "graphql-middleware": "6.1.35",
    "graphql-scalars": "1.22.4",
    "graphql-yoga": "5.0.0",
    "lodash": "4.17.21",
    "msw": "1.3.2",
    "mysql2": "^3.6.5",
    "next": "^14.0.4",
    "next-language-detector": "1.1.0",
    "next-plugin-graphql": "0.0.2",
    "pg": "^8.11.5",
    "pino": "8.16.2",
    "pino-pretty": "10.2.3",
    "pino-sentry": "0.14.0",
    "postgres": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.48.2",
    "react-icons": "4.12.0",
    "reflect-metadata": "0.1.13",
    "tslib": "2.6.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@chakra-ui/cli": "2.4.1",
    "@faker-js/faker": "8.3.1",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@graphql-tools/executor-http": "1.0.3",
    "@rollup/plugin-graphql": "2.0.4",
    "@sudolabs-io/eslint-config-sudolabs": "1.5.194",
    "@types/node": "20.9.3",
    "@types/pg": "^8.11.6",
    "@types/pino": "7.0.5",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "drizzle-kit": "^0.21.2",
    "eslint": "8.54.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "graphql-tag": "2.12.6",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "nodemailer": "6.9.7",
    "prettier": "3.1.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{md,json}": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
